{
  "name": "auto-install",
  "version": "1.7.5",
  "description": "Auto installs dependencies as you code",
  "keywords": "auto, dependencies, install, package, watch",
  "homepage": "https://github.com/siddharthkp/auto-install#readme",
  "repository": "siddharthkp/auto-install",
  "license": "MIT",
  "main": "src/index.js",
  "bin": "lib/index.js",
  "author": "siddharthkp",
  "scripts": {
    "lint": "eslint src",
    "build": "babel src --out-dir lib && babel test/src --out-dir test/lib",
    "run-tests": "cd example && mocha ../test/lib",
    "test": "npm run build && npm run run-tests"
  },
  "engines": {
    "node": ">= 4.4.5"
  },
  "dependencies": {
    "chokidar": "2.0.0",
    "colors": "1.1.2",
    "glob": "7.1.0",
    "is-builtin-module": "1.0.0",
    "log-symbols": "1.0.2",
    "ora": "0.3.0",
    "node-source-walk": "^4.2.0",
    "vue-template-compiler": "^2.6.11",
    "package-json": "2.4.0",
    "which-pm": "^2.0.0",
    "yargs": "6.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "chai": "3.5.0",
    "core-js": "^3.6.5",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.15.0",
    "mocha": "3.1.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6"
          },
          "useBuiltIns": "usage",
          "corejs": "3.6.5"
        }
      ]
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  }
}
